{
    "manifest_version": 3,
    "name": "GonePhishing",
    "version": "0.0.1",
    "description": "Detect scam and phishing websites",
    "action": {
        "default_popup": "index.html",
        "default_title": ""
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "nativeMessaging"
    ],
    "background": {
        "service_worker": "background.js"
      }
}